    listen 80;
    server_name farm.channab.com;
    access_log off;

    location /static/ {
        alias /home/ubuntu/channab/channab4/channab/static/;
    }

    location / {
        proxy_pass http://unix:/home/ubuntu/channab/channab4/channab/channab.sock;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
